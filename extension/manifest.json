{
  "manifest_version": 3,
  "name": "LinkedIn Auto Scraper",
  "version": "1.0",
  "permissions": ["tabs", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://www.linkedin.com/*"]
}
